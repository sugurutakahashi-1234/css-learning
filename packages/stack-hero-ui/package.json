{
  "name": "@css-learning/stack-hero-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "generate": "openapi-typescript ../../openapi.yaml -o src/generated/api.ts",
    "test": "vitest run",
    "lint": "biome check .",
